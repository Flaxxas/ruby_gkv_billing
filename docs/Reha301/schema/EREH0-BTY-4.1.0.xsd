	<xs:simpleType name="Text_Leist_Stp">
		<xs:annotation>
			<xs:documentation>Originäre Bezeichnung der im Einzelfall für einen Rehabilitanden durch die Reha-Einrichtung erbrachten therapeutischen Leistung, der Basis für die Verschlüsselung ist. max. 108 Zeichen erlaubt  </xs:documentation>
		</xs:annotation>
		<xs:restriction base="Token_intern_Stp">
			<xs:minLength value="0"/>
			<xs:maxLength value="108"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Therapieart_Stp">
		<xs:annotation>
			<xs:documentation>00 = Keine Bewilligung einer Therapie gem. Ziff. 1-2;01 = Therapeutische Einzel/ Gruppengespräche;02 = Therapeutische Einzel/ Gruppengespräche für Bezugspersonen</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="01"/>
			<xs:enumeration value="02"/>
			<xs:enumeration value="00"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Therapieeinheiten_99_Stp">
		<xs:annotation>
			<xs:documentation>1-99 Feld</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="2"/>
			<xs:pattern value="([0-9]*)([1-9])([0-9]*)"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Therapieeinheiten_999_Stp">
		<xs:annotation>
			<xs:documentation>1-999 Feld</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="3"/>
			<xs:pattern value="([0-9]*)([1-9])([0-9]*)"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Token_intern_Stp">
		<xs:annotation>
			<xs:documentation>entspricht dem xs:token aber ohne "Datenmanipulation". kein Tabs, keine führenden/angehängte Leerzeichen, keine mehrfachen Leerzeichen, keine Zeilenumbrüche; Ausdruck beginnt mit einem nicht-whitespace, dann folgen 0-1 Whitespaces; dies wird beliebig oft wiederholt und endet mit einem Nicht-whitespace</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="(\S ?)*\S"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Version_Stp">
		<xs:annotation>
			<xs:documentation>Version im Format 1.0.0 bis 9.999.999 </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[1-9]{1}[0-9]{0,2}\.[0-9]{1,3}\.[0-9]{1,3}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Version_ICD10_Stp">
		<xs:annotation>
			<xs:documentation>Version ICD10</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Token_intern_Stp">
			<xs:pattern value="[0-9]{4}"/>
		</xs:restriction>
