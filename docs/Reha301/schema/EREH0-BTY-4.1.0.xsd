<?xml version="1.0" encoding="ISO-8859-1"?>
<xs:schema xmlns="http://www.vdek.com/xml-schema/BTY/4.1" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.vdek.com/xml-schema/BTY/4.1" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<!--  	
            :notation  SIMPLE TYPE  ___ ALLGEMEIN
  _________________________________________________________________________________________________________________ -->
	<xs:simpleType name="Anzahl_Leist_Stp">
		<xs:annotation>
			<xs:documentation>Anzahl ein- oder zweistellig mit oder ohne führender Null: 1 - 2 Ziffern außer "0" und "00"</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="2"/>
			<xs:pattern value="([0-9]*)([1-9])([0-9]*)"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Anzahl_Tage_Stp">
		<xs:annotation>
			<xs:documentation>Zahl der Tage -  nummerisches Feld 1 bis 999</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="3"/>
			<xs:pattern value="([1-9])([0-9]*)"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Anzahl_Tage_mit_0_Stp">
		<xs:annotation>
			<xs:documentation>Zahl der Tage -  nummerisches Feld 0 bis 999</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="3"/>
			<xs:pattern value="[0-9]|([1-9][0-9]*)"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Arbeitsfaehig_KV_Stp">
		<xs:annotation>
			<xs:documentation>Aussage der Reha-Einrichtung zur Arbeitsfähigkeit des Rehabilitanden bei Entlassung; 1 = Arbeitsfähig, 3 = Arbeitsunfähig, 9 = Beurteilung nicht erforderlich  </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="1"/>
			<xs:enumeration value="3"/>
			<xs:enumeration value="9"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Arbeitsfaehig_RV_Stp">
		<xs:annotation>
			<xs:documentation>Aussage der Reha-Einrichtung zur Arbeitsfähigkeit des Rehabilitanden bei Entlassung; 1 = Arbeitsfähig, 3 = Arbeitsunfähig,  9 = Beurteilung nicht erforderlich </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="0"/>
			<xs:enumeration value="1"/>
			<xs:enumeration value="3"/>
			<xs:enumeration value="4"/>
			<xs:enumeration value="5"/>
			<xs:enumeration value="9"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Art_der_Behandlung_Stp">
		<xs:annotation>
			<xs:documentation>1 = stationär, 2 = ganztätig ambulant, 3 = ambulant, 4 = mobil</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
			<xs:enumeration value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Art_der_Behandlung_KV_Stp">
		<xs:annotation>
			<xs:documentation>1 = stationär, 2 = ganztätig ambulant, 4 = mobil, 5 = Kombinationsbehandlung</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="4"/>
			<xs:enumeration value="5"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Art_der_Behandlung_RV_Stp">
		<xs:annotation>
			<xs:documentation>1 = stationär, 2 = ganztätig ambulant, 3 = ambulant (Sucht)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Besondere_Behandlungsformen_RV_Stp">
		<xs:annotation>
			<xs:documentation>0 = keine, 1 = MBOR, 2 = VMO (vor 2021), 3 = VOR, 4 = MBOR/VOR (ab 2021), 9 = sonstige</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="0"/>
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
			<xs:enumeration value="4"/>
			<xs:enumeration value="9"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Behandlungsergebnis_Stp">
		<xs:annotation>
			<xs:documentation>Diagnosenbezogene Bewertung des Behandlungsergebnisses bei einer Rehabilitationsmaßnahme; 0 = Ziff. 1-3 trifft nicht zu, 1 = gebessert, 2 = unverändert, 3 = verschlechtert </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="0"/>
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Betrag_3_2_Stp">
		<xs:annotation>
			<xs:documentation>3 Stellen vor dem Komma, 2 Stellen Nach</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Eurocent_Stp">
			<xs:minLength value="1"/>
			<xs:maxLength value="5"/>
			<xs:pattern value="[0-9]*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Dateiart_Stp">
		<xs:annotation>
			<xs:documentation>Dateiart (z.B. pdf)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Token_intern_Stp">
			<xs:minLength value="1"/>
			<xs:maxLength value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Dateigroesse_Stp">
		<xs:annotation>
			<xs:documentation>Dateigroesse in Bytes vor Konvertierung</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="8"/>
			<xs:pattern value="([0-9]*)([1-9])([0-9]*)"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Dateikategorie_Stp">
		<xs:annotation>
			<xs:documentation>Dateikategori (z.b. MEDI, VERW)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="Token_intern_Stp">
			<xs:minLength value="2"/>
			<xs:maxLength value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Dateinummer_Stp">
		<xs:annotation>
			<xs:documentation>6 Ziffern</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9]{6}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Dauer_Leist_Stp">
		<xs:annotation>
			<xs:documentation>Zahl der für einen Rehabilitanden während einer Reha-Maßnahme durchgeführten therapeutischen Maßnahmen / Leistungen für eine bestimmte therapeutische Leistung gemäß KTL: A = 5 min, B = 10 min, C = 15 min, D = 20 min, E = 25 min, F = 30 min, G = 35 min, H = 40 min, I = 45 min, K = 50 min, L = 60 min, M = 75 min, N = 90 min, P = 100 min, Q = 120 min, R = 150 min, S = 180 min, T = 240 min, U = 300 min, W = größer/gleich 360 min, Z = individuell </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[A-I]"/>
			<xs:pattern value="[K-N]"/>
			<xs:pattern value="[P-U]"/>
			<xs:pattern value="[W]"/>
			<xs:pattern value="[Z]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Empfehlung_Stp">
		<xs:annotation>
			<xs:documentation>Eine von mehreren möglichen Empfehlungen der Reha-Einrichtung bei Abschluss einer Reha-Leistung an Rehabilitanden und weitere Stellen, bestimmte (Nachsorge-)Maßnahmen durchzuführen, und zwar an Ärzte / Psychologen, an Rehabilitanden/Patienten und an Rentenversicherung/Krankenversicherung </xs:documentation>
		</xs:annotation>
		<xs:restriction base="Token_intern_Stp">
			<xs:enumeration value="01"/>
			<xs:enumeration value="02"/>
			<xs:enumeration value="03"/>
